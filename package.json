{
  "name": "jni-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "npx eslint ./src --fix",
    "prepare": "husky install",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.13",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "axios": "^1.3.4",
    "eslint-plugin-jest-dom": "^4.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.0-beta.61",
    "@storybook/addon-interactions": "^7.0.0-beta.61",
    "@storybook/addon-links": "^7.0.0-beta.61",
    "@storybook/blocks": "^7.0.0-alpha.8",
    "@storybook/preset-create-react-app": "^7.0.0-beta.61",
    "@storybook/react": "^7.0.0-beta.61",
    "@storybook/react-webpack5": "^7.0.0-beta.61",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "babel-plugin-named-exports-order": "^0.0.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-compat": "^4.1.2",
    "eslint-plugin-jest-extended": "^2.0.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^46.0.0",
    "husky": "^8.0.0",
    "jest-extended": "^3.2.4",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.0.0-beta.61",
    "webpack": "^5.75.0"
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    },
    "coveragePathIgnorePatterns": [
      "index.tsx",
      "reportWebVitals.ts",
      "src/data",
      ".stories.*",
      ".spec.*"
    ]
  },
  "eslintConfig": {
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  }
}
